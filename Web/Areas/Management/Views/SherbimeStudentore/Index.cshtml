                            </div>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-12">
                                    @if (Model.sherbimeStudentoreKerkesaList.Count > 0)
                                    {
                                        <table id="kerkesaAnkesa-table" class="table table-bordered table-striped table-hover table-fw-widget">
                                            <thead>
                                                <tr>
                                                    <th class="text-center">Kerkesa ID</th>
                                                    <th class="text-center">Tipi i kerkeses</th>
                                                    <th class="text-center">Pershkrimi</th>
                                                    <th class="text-center">Departamenti</th>
                                                    <th class="text-center">Data e paraqitjes</th>
                                                    <th class="text-center">Eshte ankes</th>
                                                    <th class="text-center">Ankes anonime</th>
                                                    <th class="text-center">Ankesa anonimeID</th>
                                                    <th class="text-center">Veprimet</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var item in Model.sherbimeStudentoreKerkesaList)
                                                {
                                                    <tr>
                                                        <td><a class="text-center" style=" color: inherit;">@item.KerkesaAnkesaId</a></td>
                                                        <td><a class="text-center" style=" color: inherit;">@item.LlojiKerkeses</a></td>
                                                        <td><a class="text-center" style=" color: inherit;">@Html.Raw(item.PershkrimiIkerkeses)</a></td>
                                                        <td><a class="text-center" style=" color: inherit;">@item.Departamenti</a></td>
                                                        <td><a class="text-center" style=" color: inherit;">@item.InsertDate</a></td>
                                                        <td>
                                                            @if (item.Ankes == true)
                                                            {
                                                                <a class="text-center" style=" color: inherit;">Po</a>
                                                            }
                                                            else
                                                            {
                                                                <a class="text-center" style=" color: inherit;">Jo</a>
                                                            }
                                                        </td>
                                                        <td>
                                                            @if (item.IsAnonim == true)
                                                            {
                                                                <a class="text-center" style=" color: inherit;">Po</a>

                                                            }
                                                            else
                                                            {
                                                                <a class="text-center" style=" color: inherit;">Jo</a>
                                                            }
                                                        </td>
                                                        <td>
                                                            @if (item.AnonimId != null)
                                                            {
                                                                <a class="text-center" style=" color: inherit;">@item.AnonimId</a>
                                                            }
                                                            else
                                                            {
                                                                <a class="text-center" style=" color: inherit;">Jo</a>
                                                            }
                                                        </td>
                                                        <td><a class="btn mb-1 btn-primary btn-sm " href="/Management/Staff/Details/@item.KerkesaAnkesaId">Details</a> | <a class="btn mb-1 btn-danger btn-sm" onclick="ConfirmDelete('@item.KerkesaAnkesaId')" id="btnContinueDelete">Delete</a></td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    }
                                    else
                                    {
                                        <h4 class="no-products-message">Nuk ka staff</h4>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
